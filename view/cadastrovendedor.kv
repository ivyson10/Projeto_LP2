#:import MDTextField kivymd.textfields.MDTextField 
#:import ThemeManager kivymd.theming.ThemeManager 
#:import MDBottomNavigation kivymd.tabs.MDBottomNavigation 
<CadastroVendedor>:
    BoxLayout:
        orientation: 'vertical'
        padding: 10, -20, 10, 30
        spacing: 20
        MDTextField:
            hint_text: 'Nome'
            multiline: False
        MDTextField:
            hint_text: 'RG'
            multiline: False
        MDTextField:
            hint_text: 'CPF'
            multiline: False
        MDTextField:
            hint_text: 'Telefone'
            multiline: False  
        MDTextField:
            hint_text: 'Email'
            multiline: False
        MDTextField:
            hint_text: 'Data de Nascimento'
            multiline: False
        MDTextField:
            hint_text: 'Salario'
            multiline: False
        MDRaisedButton:
            text: 'Cadastrar'
            size_hint: None, None
            pos_hint: {'center_x': 0.5, 'center_y': 0.6}
        